{
  "name": "cinema",
  "version": "0.0.0",
  "description": "shared video watching",
  "scripts": {
    "build": "parcel build --public-url . src/index.html",
    "watch": "parcel watch --public-url . src/index.html --no-hmr",
    "serve": "parcel serve --port 1236 src/index.html",
    "analyze": "parcel build --public-url . src/index.html --reporter @parcel/reporter-bundle-analyzer"
  },
  "repository": "https://github.com/shish/cinema/",
  "author": "Shish <webmaster@shishnet.org>",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/shish/cinema/issues"
  },
  "homepage": "https://github.com/shish/cinema#readme",
  "dependencies": {
    "hls.js": "^1.3.0",
    "hyperapp": "^2.0.22",
    "hyperapp-fx": "^2.0.0-beta.2",
    "hyperapp-jsx-pragma": "^1.3.0",
    "jsonpatch": "^3.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "parcel": "^2.8.3",
    "@parcel/optimizer-data-url": "^2.8.3",
    "@parcel/reporter-bundle-analyzer": "^2.8.3",
    "@parcel/transformer-inline-string": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3"
  }
}
